return function(D, SIGNAL)
    local UserKB = {}

    UserKB.IsKeyDown = D.ISDOWN

    local KBProxy = setmetatable({}, { __index = UserKB, __newindex = function() end, __metatable = false })

    local DATA = {}

    DATA.GetKeyboard = function()
        return KBProxy
    end

    UserKB.KeyPressed = SIGNAL.new(SIGNAL.Type.RTC)
    UserKB.KeyReleased = SIGNAL.new(SIGNAL.Type.RTC)

    return DATA
end
